# KAMCO API Service Key
# Get your key from https://www.data.go.kr/
# Search for "한국자산관리공사_공매" and register for API access
# Use the URL-Encoded version of your service key

KAMCO_API_KEY=YOUR_ENCODED_API_KEY_HERE

# MongoDB Configuration
MONGO_URI=mongodb://localhost:27017
MONGO_DB_NAME=kamco
MONGO_COLLECTION_NAME=collected_items

# Qdrant Vector Database Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=kamco

# Ollama Model Configuration
EMBED_MODEL=qwen2.5:latest
GEN_MODEL=qwen2.5:latest

# Flask Secret Key (generate a random string for production)
FLASK_SECRET_KEY=your_random_secret_key_here

